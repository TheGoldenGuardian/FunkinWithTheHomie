function start(song) {
	var bg = new FlxSprite(-500, -170).loadGraphic(hscriptPath + 'garStagebg.png');
	bg.antialiasing = true;
	bg.scrollFactor.set(0.7, 0.7);
	bg.active = false;
	addSprite(bg, BEHIND_ALL);

	var bgAlley = new FlxSprite(-500, -200).loadGraphic(hscriptPath + 'garStage.png');
	bgAlley.antialiasing = true;
	bgAlley.scrollFactor.set(0.9, 0.9);
	bgAlley.active = false;
	addSprite(bgAlley, BEHIND_ALL);
	
    setDefaultZoom(0.9);
}


